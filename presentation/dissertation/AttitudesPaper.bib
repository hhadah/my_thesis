  shorttitle = {Racial Bias Is Associated with Ingroup Death Rate for {{Blacks}} and {{Whites}}},
  author = {Leitner, Jordan B. and Hehman, Eric and Ayduk, Ozlem and {Mendoza-Denton}, Rodolfo},
  year = {2016},
